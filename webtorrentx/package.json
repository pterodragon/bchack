{
  "name": "webtorrentx",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "start-seeder": "DEBUG=wx.* nodemon seeder.ts",
    "start-leecher": "DEBUG=wx.* nodemon leecher.ts",
    "debug-seeder": "nodemon -e ts --exec 'node --inspect-brk --require ts-node/register seeder.ts'",
    "debug-leecher": "nodemon -e ts --exec 'node --inspect-brk --require ts-node/register leecher.ts'"
  },
  "dependencies": {
    "bittorrent-dht": "^10.0.0",
    "bittorrent-protocol": "^3.2.0",
    "debug": "^4.3.1",
    "webtorrent": "^0.109.0"
  },
  "devDependencies": {
    "@types/bittorrent-protocol": "^3.1.1",
    "@types/debug": "^4.1.5",
    "@types/webtorrent": "^0.109.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
